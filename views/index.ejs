<%- include ('partials/header')  %>
    <% blogs.forEach(blog => { %>
        <div class="container main">
            <h2>
                <%= blog.title %>
            </h2>
            <img src="<%= blog.image %> " alt="">
            <span><%= blog.created %> </span>
            <p>
                <%= blog.body %>
            </p>
        </div>
        <% }); %>
            <%- include ('partials/footer')  %>